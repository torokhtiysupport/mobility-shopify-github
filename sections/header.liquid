{% comment %}theme-check-disable ImgLazyLoading{% endcomment %}
{%- liquid
  assign has_localization = false
  assign total_location = 0
  if section.settings.enable_country_selector and localization.available_countries.size > 1
    assign has_localization = true
    assign total_location = total_location | plus: 1
  endif
  if section.settings.enable_language_selector and localization.available_languages.size > 1
    assign has_localization = true
    assign total_location = total_location | plus: 1
  endif  
  assign menu_labels = section.blocks | where: 'type', 'menu_label'
  assign has_transparent = false
  if section.settings.enable_transparent_homepage and request.page_type == 'index'
    assign has_transparent = true
  endif
  render 'header-assets', has_localization: has_localization, menu_labels: menu_labels, has_transparent: has_transparent, text_color_transparent: section.settings.text_color_transparent
  assign store_information_blocks = section.blocks | where: 'type', 'store_information'
  if shop.customer_accounts_enabled and section.settings.mobile_logo_position == 'center'
    assign has_left_account_icon = true
  endif
  if settings.social_twitter_link != blank or settings.social_facebook_link != blank or settings.social_pinterest_link != blank or settings.social_instagram_link != blank or settings.social_tiktok_link != blank or settings.social_tumblr_link != blank or settings.social_snapchat_link != blank or settings.social_youtube_link != blank or settings.social_vimeo_link != blank
    assign has_social_media_icons = true
  endif
  assign has_supported_region = false
  assign supported_region_blocks = section.blocks | where: 'type', 'customer_supported_region'
  if section.settings.customer_support_region_global_hotline != blank
    assign has_supported_region = true
  endif
-%}

<{% if section.settings.enable_sticky_header %}sticky-header{% else %}div{% endif %} class="header-wrapper por{% if section.settings.enable_sticky_header %} db{% endif %} color-{{ section.settings.color_scheme }} gradient{% if section.settings.show_line_separator %} header-wrapper--border-bottom{% endif %}{% if section.settings.enable_sticky_header and section.settings.logo_position == 'top-left' %} toggle-nav{% endif %}"{% if section.settings.enable_sticky_header and section.settings.logo_position == 'top-left' %} toggle-nav{% endif %}"{% if has_transparent %} data-transparent="1"{% endif %} data-sticky-type="{{ section.settings.sticky_header }}">
  {%- if store_information_blocks.size > 0 or has_localization or section.settings.enable_social_links -%}
    <div class="header-top color-{{ section.settings.top_color_scheme }} isolate gradient">
      <div class="page-width">
        <div class="flex flex-wrap w100 justify-space-between align-center">
          {%- if store_information_blocks.size > 0 -%}
            <div class="header__store-information small-hide medium-hide">
              {%- render 'header-store-information', store_information_blocks: store_information_blocks -%}
            </div>
          {%- endif -%}
          {%- if has_localization or section.settings.enable_social_links -%}
            <div class="header__localization align-center isolate flex {% if total_location == 2 %}justify-space-between{% else %}justify-right{% endif %} flex-grow-1">
              {%- if section.settings.enable_country_selector and localization.available_countries.size > 1 -%}
                <noscript>
                  {%- form 'localization', id: 'HeaderCountryFormNoScript', class: 'localization-form' -%}
                    <div class="localization-form__select">
                      <h2 class="visually-hidden" id="HeaderCountryLabelNoScript">{{ 'localization.country_label' | t }}</h2>
                      <select class="localization-selector link" name="country_code" aria-labelledby="HeaderCountryLabelNoScript">
                        {%- for country in localization.available_countries -%}
                          <option value="{{ country.iso_code }}" {%- if country.iso_code == localization.country.iso_code %} selected{% endif %}>
                            {{ country.name }} ({{ country.currency.iso_code }} {{ country.currency.symbol }})
                          </option>
                        {%- endfor -%}
                      </select>
                      {% render 'icon-caret' %}
                    </div>
                    <button class="button button--tertiary"><span>{{ 'localization.update_country' | t }}</span></button>
                  {%- endform -%}
                </noscript>
                <localization-form>
                  {%- form 'localization', id: 'HeaderCountryForm', class: 'localization-form' -%}
                    <details-disclosure class="no-js-hidden" data-ignore-hover="true">
                      <h2 class="text-small text-body visually-hidden" id="HeaderCountryLabel">{{ 'localization.country_label' | t }}</h2>
                      <details class="disclosure por">
                        <summary class="disclosure__button localization-form__select localization-selector link link--text flex align-center left por" aria-expanded="false" aria-controls="HeaderCountryList" aria-describedby="HeaderCountryLabel">
                          {{ localization.country.name }} ({{ localization.country.currency.iso_code }} {{ localization.country.currency.symbol }})
                          {% render 'icon-caret' %}
                        </summary>
                        <div class="disclosure__list-wrapper disclosure__list-wrapper--bottom">
                          <ul id="HeaderCountryList" role="list" class="disclosure__list list-unstyled">
                            {%- for country in localization.available_countries -%}
                              <li class="disclosure__item" tabindex="-1">
                                <a class="link link--text disclosure__link text-small{% if country.iso_code == localization.country.iso_code %} disclosure__link--active{% endif %}" href="#"{% if country.iso_code == localization.country.iso_code %} aria-current="true"{% endif %} data-value="{{ country.iso_code }}">
                                  {{ country.name }} <span class="localization-form__currency">({{ country.currency.iso_code }} {{ country.currency.symbol }})</span>
                                </a>
                              </li>
                            {%- endfor -%}
                          </ul>
                        </div>
                      </details>
                      <input type="hidden" name="country_code" value="{{ localization.country.iso_code }}">
                    </details-disclosure>
                  {%- endform -%}
                </localization-form>
              {%- endif -%}
        
              {%- if section.settings.enable_language_selector and localization.available_languages.size > 1 -%}
                <noscript>
                  {%- form 'localization', id: 'HeaderLanguageFormNoScript', class: 'localization-form' -%}
                    <div class="localization-form__select">
                      <h2 class="visually-hidden" id="HeaderLanguageLabelNoScript">{{ 'localization.language_label' | t }}</h2>
                      <select class="localization-selector link" name="locale_code" aria-labelledby="HeaderLanguageLabelNoScript">
                        {%- for language in localization.available_languages -%}
                          <option value="{{ language.iso_code }}" lang="{{ language.iso_code }}" {%- if language.iso_code == localization.language.iso_code %} selected{% endif %}>
                            {{ language.endonym_name | capitalize }}
                          </option>
                        {%- endfor -%}
                      </select>
                      {% render 'icon-caret' %}
                    </div>
                    <button class="button button--tertiary"><span>{{ 'localization.update_language' | t }}</span></button>
                  {%- endform -%}
                </noscript>
        
                <localization-form>
                  {%- form 'localization', id: 'HeaderLanguageForm', class: 'localization-form' -%}
                    <details-disclosure class="no-js-hidden" data-ignore-hover="true">
                      <h2 class="text-small text-body visually-hidden" id="HeaderLanguageLabel">{{ 'localization.language_label' | t }}</h2>
                      <details class="disclosure por">
                        <summary class="disclosure__button flex align-center localization-form__select localization-selector link link--text flex align-center left por" aria-expanded="false" aria-controls="HeaderLanguageList" aria-describedby="HeaderLanguageLabel">
                          {{ localization.language.endonym_name | capitalize }}
                          {% render 'icon-caret' %}
                        </summary>
                        <div class="disclosure__list-wrapper disclosure__list-wrapper--bottom disclosure__list-wrapper--header">
                          <ul id="HeaderLanguageList" role="list" class="disclosure__list list-unstyled">
                            {%- for language in localization.available_languages -%}
                              <li class="disclosure__item" tabindex="-1">
                                <a class="link link--text disclosure__link text-small{% if language.iso_code == localization.language.iso_code %} disclosure__link--active{% endif %}" href="#" hreflang="{{ language.iso_code }}" lang="{{ language.iso_code }}"{% if language.iso_code == localization.language.iso_code %} aria-current="true"{% endif %} data-value="{{ language.iso_code }}">
                                  {{ language.endonym_name | capitalize }}
                                </a>
                              </li>
                            {%- endfor -%}
                          </ul>
                        </div>
                      </details>
                      <input type="hidden" name="locale_code" value="{{ localization.language.iso_code }}">
                    </details-disclosure>
                  {%- endform -%}
                </localization-form>
              {%- endif -%}

              {%- if section.settings.enable_social_links -%}
                {%- render 'social-media-icons', add_classes: 'list small-hide medium-hide list-social--header', icon_width: 16 -%}
              {%- endif -%}  
            </div>
          {%- endif -%}
        </div>
      </div>
    </div>
  {%- endif -%}
  <header class="header header--{{ section.settings.logo_position }} w100{% if section.settings.menu != blank %} header--has-menu{% endif %} header--mobile-{{ section.settings.mobile_logo_position }}">
    <div class="header__inner page-width">  
      {%- if section.settings.menu != blank or has_left_account_icon or has_social_media_icons or has_supported_region or settings.enable_product_compare -%}
        {%- liquid
          comment
            Collect the mega menu blocks and menu badge
          endcomment
          assign blocks_str = ''
          assign badges_str = ''
          for block in section.blocks
            if block.type contains 'mega_menu' and block.settings.trigger != blank
              assign compare_index = ':' | append: block.settings.trigger | append: '=' | downcase
              if blocks_str contains compare_index
                assign exist_blocks = blocks_str | split: compare_index | last | split: ';' | first
                assign new_blocks = exist_blocks | append: ',' | append: forloop.index0
                assign find_str = compare_index | append: exist_blocks | append: ';'
                assign replace_str = compare_index | append: new_blocks | append: ';'
                assign blocks_str = blocks_str | replace: find_str, replace_str
              else
                assign blocks_str = blocks_str | append: compare_index | append: forloop.index0 | append: ';'
              endif
            endif
            if block.type == 'menu_badge'
              assign compare_index = ':' | append: block.settings.trigger | append: '=' | downcase
              if badges_str contains compare_index
                assign exist_blocks = badges_str | split: compare_index | last | split: ';' | first
                assign new_blocks = exist_blocks | append: ',' | append: forloop.index0
                assign find_str = compare_index | append: exist_blocks | append: ';'
                assign replace_str = compare_index | append: new_blocks | append: ';'
                assign badges_str = badges_str | replace: find_str, replace_str
              else
                assign badges_str = badges_str | append: compare_index | append: forloop.index0 | append: ';'
              endif
            endif
          endfor
        -%}
        <div class="header__left-icons flex justify-left{% unless section.settings.logo_position == 'center-drawer' %} large-up-hide{% endunless %}">
          <header-drawer data-breakpoint="{% unless section.settings.logo_position == 'center-drawer' %}tablet{% else %}all{% endunless %}" class="header__drawer-mobile align-self-center" id="header-drawer-menu-mobile">
            <details id="Details-menu-drawer-container" class="menu-drawer-container">
              <summary class="header__icon header__icon--menu header__icon--summary link header__button-icon flex justify-center align-center" aria-label="{{ 'sections.header.menu' | t }}">
                <span class="por">
                  {%-liquid
                    render 'icon-hamburger', add_classes: 'icon-open-state' 
                    render 'icon-close', add_classes: 'icon-close-state'
                  -%}
                </span>
              </summary>
              <div id="menu-drawer" class="gradient menu-drawer menu-drawer--left motion-reduce" tabindex="-1">
                <div class="menu-drawer__inner-container por h100">
                  <div class="menu-drawer__navigation-container h100 overflow-hidden">
                    {%- if section.settings.menu != blank -%}
                      <nav class="menu-drawer__navigation menu-drawer__block">
                        <ul class="menu-drawer__menu has-submenu list-menu menu-drawer__background" role="list">
                          {%- for link in section.settings.menu.links -%}
                            {%- assign compare_index = ':' | append: link.title | append: '=' | downcase -%}
                            <li>
                              {%- if link.links != blank or blocks_str contains compare_index -%}
                                <details id="Details-menu-drawer-menu-item-{{ forloop.index }}">
                                  <summary class="menu-drawer__menu-item list-menu__item link link--text header__menu-item--top h6{% if link.child_active or link.current %} menu-drawer__menu-item--active{% endif %}{% if forloop.first %} first{% endif %}{% if forloop.last %} last{% endif %} w100">
                                    {{ link.title }}
                                    {%- liquid
                                      render 'icon-caret'
                                      if badges_str contains compare_index
                                        assign blocks_index = badges_str | split: compare_index | last | split: ';' | first
                                        render 'header-menu-badge', blocks_index: blocks_index, is_in_drawer: true
                                      endif
                                    -%}
                                  </summary>
                                  {%- liquid
                                    assign first_column = ''
                                    assign last_column = ''
                                    if blocks_str contains compare_index
                                      assign blocks_index = blocks_str | split: compare_index | last | split: ';' | first
                                      assign blocks = blocks_index | split: ','
                                      for block_index in blocks
                                        assign block_index_number = block_index | plus: 0
                                        assign block = section.blocks[block_index_number]
                                        capture block_html
                                          if block.type == 'mega_menu_promotion'
                                            render 'header-megamenu-promotion-block' with block: block, add_classes: 'menu-drawer__menu-item', no_block_attributes: true
                                          elsif block.type == 'mega_menu_product_brand'
                                            render 'header-megamenu-product-brand-block' with block: block, add_classes: 'menu-drawer__menu-item', no_block_attributes: true
                                          elsif block.type == 'mega_menu_product_type'
                                            render 'header-megamenu-product-type-block' with block: block, add_classes: 'menu-drawer__menu-item', no_block_attributes: true  
                                          endif
                                        endcapture
                                        case block.settings.position
                                          when 'first'
                                            assign first_column = first_column | append: block_html
                                          when 'last'
                                            assign last_column = last_column | append: block_html
                                        endcase
                                      endfor
                                    endif  
                                  -%}
                                  <div id="link-{{ link.handle | escape }}" class="menu-drawer__submenu has-submenu gradient motion-reduce" tabindex="-1">
                                    <div class="menu-drawer__inner-submenu menu-drawer__background">
                                      <button class="menu-drawer__close-button link link--text" aria-expanded="true">
                                        {% render 'icon-arrow' %}
                                        {{ link.title }}
                                      </button>
                                      <ul class="menu-drawer__menu list-menu" role="list" tabindex="-1">
                                        {%- if link.url and section.settings.show_go_to_link -%}
                                          <li>
                                            <a href="{{ link.url }}" class="menu-drawer__menu-item link link--text list-menu__item{% if link.current %} menu-drawer__menu-item--active{% endif %} w100"{% if link.current %} aria-current="page"{% endif %}>
                                              {{ 'sections.header.go_to_link' | t: title: link.title }}
                                            </a>
                                          </li>
                                        {%- endif -%}  
                                        {%- if first_column != '' -%}
                                          <li>{{ first_column }}</li>
                                        {%- endif -%}
                                        {%- for childlink in link.links -%}
                                          <li>
                                            {%- if childlink.links == blank -%}
                                              <a href="{{ childlink.url }}" class="menu-drawer__menu-item link link--text list-menu__item{% if childlink.child_active or childlink.current %} menu-drawer__menu-item--active{% endif %}{% if forloop.last %} last{% endif %} w100"{% if childlink.current %} aria-current="page"{% endif %}>
                                                {{ childlink.title }}
                                              </a>
                                            {%- else -%}
                                              <details id="Details-menu-drawer-submenu-{{ forloop.index }}">
                                                <summary class="menu-drawer__menu-item link link--text list-menu__item{% if childlink.child_active or childlink.current %} menu-drawer__menu-item--active{% endif %}{% if forloop.last %} last{% endif %} w100">
                                                  {{ childlink.title }}
                                                  {% render 'icon-caret' %}
                                                </summary>
                                                <div id="childlink-{{ childlink.handle | escape }}" class="menu-drawer__submenu has-submenu gradient motion-reduce">
                                                  <div class="menu-drawer__inner-submenu menu-drawer__background">
                                                    <button class="menu-drawer__close-button link link--text" aria-expanded="true">
                                                      {% render 'icon-arrow' %}
                                                      {{ childlink.title }}
                                                    </button>
                                                    <ul class="menu-drawer__menu list-menu" role="list" tabindex="-1">
                                                      {%- if childlink.url and section.settings.show_go_to_link -%}
                                                        <li>
                                                          <a href="{{ childlink.url }}" class="menu-drawer__menu-item link link--text list-menu__item{% if childlink.current %} menu-drawer__menu-item--active{% endif %}{% if forloop.last %} last{% endif %} w100"{% if childlink.current %} aria-current="page"{% endif %}>
                                                            {{ 'sections.header.go_to_link' | t: title: childlink.title }}
                                                          </a>
                                                        </li>
                                                      {%- endif -%}
                                                      {%- for grandchildlink in childlink.links -%}
                                                        <li>
                                                          <a href="{{ grandchildlink.url }}" class="menu-drawer__menu-item link link--text list-menu__item{% if grandchildlink.current %} menu-drawer__menu-item--active{% endif %}{% if forloop.last %} last{% endif %} w100"{% if grandchildlink.current %} aria-current="page"{% endif %}>
                                                            {{ grandchildlink.title }}
                                                          </a>
                                                        </li>
                                                      {%- endfor -%}
                                                    </ul>
                                                  </div>
                                                </div>
                                              </details>
                                            {%- endif -%}
                                          </li>
                                        {%- endfor -%}
                                        {%- if last_column != '' -%}
                                          <li>{{ last_column }}</li>
                                        {%- endif -%}
                                      </ul>
                                    </div>
                                  </div>
                                </details>
                              {%- else -%}
                                <a href="{{ link.url }}" class="menu-drawer__menu-item list-menu__item link link--text header__menu-item--top h6{% if link.current %} menu-drawer__menu-item--active{% endif %}{% if forloop.first %} first{% endif %}{% if forloop.last %} last{% endif %} w100{% if badges_str contains compare_index %} por{% endif %}"{% if link.current %} aria-current="page"{% endif %}>
                                  {{ link.title | escape }}
                                  {%- liquid
                                    if badges_str contains compare_index
                                      assign blocks_index = badges_str | split: compare_index | last | split: ';' | first
                                      render 'header-menu-badge', blocks_index: blocks_index, is_in_drawer: true
                                    endif
                                  -%}
                                </a>
                              {%- endif -%}
                            </li>
                          {%- endfor -%}
                        </ul>
                      </nav>
                    {%- endif -%}
                    {%- if has_supported_region -%}
                      <div class="menu-drawer__block menu-drawer__background">
                        {%- if supported_region_blocks.size > 0 -%}
                          <details-accordion id="header-region-mobile">
                            <details>
                              <summary class="header__customer-support-region__button header__customer-support-region__button--mobile flex align-center por no-control-menu link link--text">
                                <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" focusable="false" role="presentation" class="icon icon-support-region light">
                                  <path d="M2 10L5 10C5.55228 10 6 10.4477 6 11V16C6 16.5523 5.55228 17 5 17H3C2.44772 17 2 16.5523 2 16V10Z" stroke="currentColor" stroke-width="2"/>
                                  <path d="M22 10L19 10C18.4477 10 18 10.4477 18 11V16C18 16.5523 18.4477 17 19 17H21C21.5523 17 22 16.5523 22 16V10Z" stroke="currentColor" stroke-width="2"/>
                                  <path d="M21 8C21 4.5 17.5228 1 12 1C6.47715 1 3 4.5 3 8" stroke="currentColor" stroke-width="2"/>
                                  <path d="M21 20V21V21C21 22.1046 20.1046 23 19 23H11.5" stroke="currentColor" stroke-width="2" stroke-linecap="round"/>
                                </svg>
                                <span class="header__customer-support-region__global-info">
                                  {%- if section.settings.customer_support_region_caption != blank -%}
                                    <span class="light db w100 caption lh1">{{ section.settings.customer_support_region_caption }}</span>
                                  {%- endif -%}
                                  <span class="font-body-bold">{{ section.settings.customer_support_region_global_hotline }}</span>
                                </span>
                                {%- render 'icon-caret-18', add_classes: 'light' -%}
                              </summary>
                              <div class="header__customer-support-region__list flex flex-column">
                                {%- for region in supported_region_blocks -%}
                                  {%- unless forloop.first -%}
                                    <hr class="margin0">
                                  {%- endunless -%}  
                                  <div class="header__customer-support-region__region flex flex-column">
                                    <span class="header__customer-support-region__name">{{ region.settings.title }}</span>
                                    <a href="tel:{{ region.settings.hotline }}" class="header__customer-support-region__hotline font-body-bold link link--text">{{ region.settings.hotline }}</a>
                                    <span class="header__customer-support-region__address light">{{ region.settings.address }}</span>
                                    <a href="mailto:{{ region.settings.email }}" class="header__customer-support-region__email link link--text">{{ region.settings.email }}</a>
                                  </div>
                                {%- endfor -%}  
                              </div>
                            </details>
                          </details-accordion>
                        {%- else -%}
                          <a class="header__customer-support-region__button flex link link--text align-center por no-control-menu" href="tel:{{ section.settings.customer_support_region_global_hotline | escape }}">
                            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" focusable="false" role="presentation" class="icon icon-support-region light">
                              <path d="M2 10L5 10C5.55228 10 6 10.4477 6 11V16C6 16.5523 5.55228 17 5 17H3C2.44772 17 2 16.5523 2 16V10Z" stroke="currentColor" stroke-width="2"/>
                              <path d="M22 10L19 10C18.4477 10 18 10.4477 18 11V16C18 16.5523 18.4477 17 19 17H21C21.5523 17 22 16.5523 22 16V10Z" stroke="currentColor" stroke-width="2"/>
                              <path d="M21 8C21 4.5 17.5228 1 12 1C6.47715 1 3 4.5 3 8" stroke="currentColor" stroke-width="2"/>
                              <path d="M21 20V21V21C21 22.1046 20.1046 23 19 23H11.5" stroke="currentColor" stroke-width="2" stroke-linecap="round"/>
                            </svg>
                            <span class="header__customer-support-region__global-info">
                              {%- if section.settings.customer_support_region_caption != blank -%}
                                <span class="light db w100 caption lh1">{{ section.settings.customer_support_region_caption }}</span>
                              {%- endif -%}
                              <span class="font-body-bold">{{ section.settings.customer_support_region_global_hotline }}</span>
                            </span>
                          </a>  
                        {%- endif -%}
                      </div>
                    {%- endif -%}

                    {%- if menu_labels.size > 0 -%}
                      <div class="menu-drawer__menu-labels flex justify-center align-center menu-drawer__block">
                        <ul class="menu-drawer__menu-labels__list list-unstyled flex margin0">
                          {%- for label in menu_labels -%}
                            <li class="menu-drawer__menu-labels__item{% unless label.settings.link != blank %} flex align-center color-foreground menu-drawer__menu-labels__item-gap{% endunless %}">
                              {%- if label.settings.link != blank -%}
                                <a href="{{ label.settings.link }}" class="link align-center flex menu-drawer__menu-labels__item-gap">
                              {%- endif -%}    
                              {%- render 'icon-picker', icon: label.settings.icon, icon_width: 16, add_classes: 'light' -%}
                              <span class="text-small">{{ label.settings.title }}</span>
                              {%- if label.settings.link != blank -%}
                                </a>
                              {%- endif -%}  
                            </li>
                          {%- endfor -%}  
                        </ul>
                      </div>
                    {%- endif -%}  

                    <div class="menu-drawer__utility-links menu-drawer__block menu-drawer__background">
                      {%- render 'social-media-icons', add_classes: 'list list-social--in-drawer', has_link_bkg: true -%}
                    </div>
                  </div>
                </div>
              </div>
            </details>
          </header-drawer>
          {%- if shop.customer_accounts_enabled and section.settings.mobile_logo_position == 'center' -%}
            <a href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}" class="header__icon header__icon--account link header__button-icon flex justify-center align-center{% if section.settings.logo_position == 'center-drawer' %} large-up-hide{% endif %}">
              {% render 'icon-account' %}
              <span class="visually-hidden">
                {%- liquid
                  if customer
                    echo 'customer.account_fallback' | t
                  else
                    echo 'customer.log_in' | t
                  endif
                -%}
              </span>
            </a>
          {%- endif -%}
        </div>  
      {%- endif -%}

      <div class="header__heading flex align-center">
        {%- if section.settings.logo_position == 'top-left' and section.settings.enable_sticky_header -%}
          <button type="button" class="header__toggle-nav-button flex justify-left align-center color-foreground header__button-icon small-hide medium-hide por" tabindex="-1" aria-label="{{ 'accessibility.navigation_toggle' | t | escape }}">
            {%-liquid
              render 'icon-hamburger', add_classes: 'icon-open-state' 
              render 'icon-close', add_classes: 'icon-close-state'
            -%}
          </button>
        {%- endif -%}  
        {%- if request.page_type == 'index' -%}
          <h1 class="margin0">
        {%- endif -%}
          <a href="{{ routes.root_url }}" class="header__heading-link link link--text flex{% if section.settings.mobile_logo_position == 'center' %} justify-center{% endif %}{% if section.settings.logo == blank %} break{% endif %}">
            {%- if section.settings.logo != blank -%}
              {%-liquid
                assign logo_width = section.settings.logo_width | plus: 30
                assign logo_alt = section.settings.logo.alt | default: shop.name | escape
                assign logo_height = logo_width | divided_by: section.settings.logo.aspect_ratio
                assign logo_classes = 'header__heading-logo motion-reduce'
                if section.settings.logo_in_transparent != blank
                  if has_transparent or request.page_type == 'collection'
                    assign logo_classes = logo_classes | append: ' hide-in-transparent'
                  endif
                endif
              -%}
              
              {{ section.settings.logo | image_url: width: 500 | image_tag:
                class: logo_classes,
                widths: '50, 100, 150, 200, 250, 300, 400, 500',
                height: logo_height,
                width: logo_width,
                alt: logo_alt
              }}
              {%- if section.settings.logo_in_transparent != blank -%}
                {%- if has_transparent or request.page_type == 'collection' -%}
                  {%-liquid
                    assign logo_alt = section.settings.logo_in_transparent.alt | default: shop.name | escape
                    assign logo_height = logo_width | divided_by: section.settings.logo_in_transparent.aspect_ratio
                    assign logo_classes = 'header__heading-logo show-in-transparent'
                  -%}
                  {{ section.settings.logo_in_transparent | image_url: width: 500 | image_tag:
                    class: logo_classes,
                    widths: '50, 100, 150, 200, 250, 300, 400, 500',
                    height: logo_height,
                    width: logo_width,
                    alt: logo_alt
                  }}
                {%- endif -%}  
              {%- endif -%}  
            {%- else -%}
              <span class="h2">{{ shop.name }}</span>
            {%- endif -%}
          </a>
        {%- if request.page_type == 'index' -%}
          </h1>
        {%- endif -%}
      </div>

      {%- if section.settings.logo_position == 'top-left' -%}
        {%- render 'header-icons', has_supported_region: has_supported_region, supported_region_blocks: supported_region_blocks -%}
      {%- endif -%}

      <div class="header__inline-menu small-hide medium-hide{% if section.settings.logo_position == 'top-left' and menu_labels.size > 0 %} w100 flex justify-space-between{% endif %}">
        {%- if section.settings.menu != blank and section.settings.logo_position != 'center-drawer' -%}
          <nav>
            <ul class="list-menu list-menu--inline" role="list">
              {%- for link in section.settings.menu.links -%}
                <li>
                  {%- assign compare_index = ':' | append: link.title | append: '=' | downcase -%}
                  {%- if link.links != blank or blocks_str contains compare_index -%}
                    <header-menu>
                      <details id="Details-HeaderMenu-{{ forloop.index }}" class="mega-menu">
                        <summary class="header__menu-item header__menu-item--top list-menu__item link h6{% if link.url != blank and link.url != '#' and link.url != '' and link.current == false %} header__menu-item--link{% endif %}" data-href="{{ link.url }}">
                          <span class="{% if link.child_active or link.current %}header__active-menu-item underlined-link{% else %}link-effect-no-underline{% endif %}">{{ link.title | escape }}</span>
                          {%-liquid
                            render 'icon-caret', add_classes: 'light'
                            if badges_str contains compare_index
                              assign blocks_index = badges_str | split: compare_index | last | split: ';' | first
                              render 'header-menu-badge', blocks_index: blocks_index
                            endif
                          -%}
                        </summary>
                        {%- render 'header-megamenu', link: link, blocks_str: blocks_str, compare_index: compare_index, index: forloop.index -%}
                      </details>
                    </header-menu>
                  {%- else -%}
                    <a href="{{ link.url }}" class="header__menu-item header__menu-item--top header__menu-item list-menu__item link h6{% if badges_str contains compare_index %} por{% endif %}"{% if link.current %} aria-current="page"{% endif %}>
                      <span class="{% if link.current %}header__active-menu-item underlined-link{% else %}link-effect-no-underline{% endif %}">{{ link.title | escape }}</span>
                      {%-liquid
                        if badges_str contains compare_index
                          assign blocks_index = badges_str | split: compare_index | last | split: ';' | first
                          render 'header-menu-badge', blocks_index: blocks_index
                        endif
                      -%}
                    </a>
                  {%- endif -%}
                </li>
              {%- endfor -%}
            </ul>
          </nav>
        {%- endif -%}

        {%- if section.settings.logo_position == 'top-left' and menu_labels.size > 0 -%}
          <div class="header__menu-labels align-self-center small-hide medium-hide">
            <ul class="list-unstyled flex margin0">
              {%- for label in menu_labels -%}
                <li{% unless label.settings.link != blank %} class="header__menu-labels__item flex align-center color-foreground"{% endunless %}>
                  {%- if label.settings.link != blank -%}
                    <a href="{{ label.settings.link }}" class="flex header__menu-labels__item link align-center">
                  {%- endif -%}    
                  {%- render 'icon-picker', icon: label.settings.icon, icon_width: 16, add_classes: 'light' -%}
                  <span class="text-small">{{ label.settings.title }}</span>
                  {%- if label.settings.link != blank -%}
                    </a>
                  {%- endif -%}  
                </li>
              {%- endfor -%}  
            </ul>
          </div>
        {%- endif -%}  
      </div>

      {%- unless section.settings.logo_position == 'top-left' -%}
        {%- render 'header-icons', has_supported_region: has_supported_region, supported_region_blocks: supported_region_blocks -%}
      {%- endunless -%}  
    </div>  
  </header>
</{% if section.settings.enable_sticky_header %}sticky-header{% else %}div{% endif %}> 

<script type="application/ld+json">
  {
    "@context": "http://schema.org",
    "@type": "Organization",
    "name": {{ shop.name | json }},
    {% if section.settings.logo %}
      "logo": {{ section.settings.logo | image_url: width: section.settings.logo.width | prepend: "https:" | json }},
    {% endif %}
    "sameAs": [
      {{ settings.social_twitter_link | json }},
      {{ settings.social_facebook_link | json }},
      {{ settings.social_pinterest_link | json }},
      {{ settings.social_instagram_link | json }},
      {{ settings.social_tiktok_link | json }},
      {{ settings.social_tumblr_link | json }},
      {{ settings.social_snapchat_link | json }},
      {{ settings.social_youtube_link | json }},
      {{ settings.social_vimeo_link | json }}
    ],
    "url": {{ request.origin | append: page.url | json }}
  }
</script>

{%- if request.page_type == 'index' -%}
  {% assign potential_action_target = request.origin | append: routes.search_url | append: "?q={search_term_string}" %}
  <script type="application/ld+json">
    {
      "@context": "http://schema.org",
      "@type": "WebSite",
      "name": {{ shop.name | json }},
      "potentialAction": {
        "@type": "SearchAction",
        "target": {{ potential_action_target | json }},
        "query-input": "required name=search_term_string"
      },
      "url": {{ request.origin | append: page.url | json }}
    }
  </script>
{%- endif -%}
{%- if request.design_mode -%}
  <script src="{{ 'theme-editor.js' | asset_url }}" defer="defer"></script>
{%- endif -%}
