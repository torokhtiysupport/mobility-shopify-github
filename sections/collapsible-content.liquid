{{ 'component-accordion.css' | asset_url | stylesheet_tag }}
{{ 'collapsible-content.css' | asset_url | stylesheet_tag }}
<script src="{{ 'details-accordion.js' | asset_url }}" defer="defer"></script>
{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
  {%- render 'section-heading-styles' -%}
{%- endstyle -%}
{%- capture sizes -%}min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 100 | divided_by: 2 }}px, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2){%- endcapture -%}
<div class="{% unless section.settings.enable_section_box_layout %}color-{{ section.settings.color_scheme }} gradient isolate{% else %}page-width{% if section.settings.section_width == 'stretched_grid' %}--full{% endif %}{% if section.settings.section_width == 'narrow' %} page-width--narrow{% endif %}{% endunless %}">
  <div class="collapsible-content collapsible-row-layout section-{{ section.id }}-padding">
    <div class="collapsible-content__wrapper {% if section.settings.enable_section_box_layout %}color-{{ section.settings.color_scheme }} gradient isolate section__box{% else %}page-width{% if section.settings.section_width == 'stretched_grid' %}--full{% endif %}{% if section.settings.section_width == 'narrow' %} page-width--narrow{% endif %}{% endif %}">
      <div class="collapsible-content__header left">
        {%- if section.settings.title != blank -%}
          {%- render 'section-heading' -%}
        {%- else -%}
          <h2 class="visually-hidden">{{ 'accessibility.collapsible_content_title' | t }}</h2>
        {% endif %}
      </div>
      <div class="grid grid--1-col{% if section.settings.image != blank %} grid--2-col-tablet{% endif %} collapsible-content__grid{% if section.settings.desktop_layout == 'image_second' %} collapsible-content__grid--reverse{% endif %}">
        {%- if section.settings.image != blank -%}
          <div class="grid__item collapsible-content__grid-item collapsible-content__image{% if section.settings.hide_image_on_mobile %} small-hide{% endif %}">
            <{% if settings.enable_appear_effect %}use-animate{% else %}div{% endif %} class="collapsible-content__media collapsible-content__media--{{ section.settings.image_ratio }} media gradient"
              {% if section.settings.image_ratio == 'adapt' %} style="padding-bottom: {{ 1 | divided_by: section.settings.image.aspect_ratio | times: 100 }}%;"{% endif %}
            >
              {{ section.settings.image | image_url: width: 1500 | image_tag:
                loading: 'lazy',
                width: section.settings.image.width,
                height: section.settings.image.height,
                sizes: sizes,
                widths: '165, 360, 535, 750, 1070, 1250, 1500, 1900, 2500'
              }}
            </{% if settings.enable_appear_effect %}use-animate{% else %}div{% endif %}>
          </div>
        {% endif %}
        <div class="grid__item">
          {%- for block in section.blocks -%}
            <details-accordion class="accordion db collapsible-content__item" {{ block.shopify_attributes }}>
              <details id="Details-{{ block.id }}-{{ section.id }}"{% if section.settings.open_first_collapsible_row and forloop.first %} open{% endif %}>
                <summary class="summary-accordion summary-accordion--has-hover flex justify-space-between align-center por">
                  <div class="summary__title flex align-center flex-grow-1">
                    {% render 'icon-accordion', icon: block.settings.icon %}
                    <h2 class="h6 accordion__title font-heading-bold margin0">
                      {{ block.settings.heading | default: block.settings.page.title }}
                    </h2>
                  </div>
                  <div class="summary-accordion__plus-minus-wrapper flex justify-center align-center">
                    <span class="summary-accordion__plus-minus por flex justify-center align-center">
                      <span class="summary-accordion__icon-line"></span>
                      <span class="summary-accordion__icon-line"></span>
                    </span>
                  </div>
                </summary>
                <div class="accordion__content{% if block.settings.icon == 'none' %} accordion__content--no-icon{% endif %}{% if block.settings.content == blank and block.settings.page.content == blank %} accordion__content--empty{% endif %}" id="CollapsibleAccordion-{{ block.id }}-{{ section.id }}" role="region" aria-labelledby="Summary-{{ block.id }}-{{ section.id }}">
                  <div class="rte">
                    {{ block.settings.row_content }}
                    {{ block.settings.page.content }}
                  </div>  
                </div>
              </details>
            </details-accordion>
          {%- endfor -%}
        </div>
      </div>
    </div>
  </div>
</div>

