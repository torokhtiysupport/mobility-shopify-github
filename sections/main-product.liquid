<section id="MainProduct-{{ section.id }}" class="product-section section-{{ section.id }}-padding{% if settings.page_width > 1400 and section.settings.media_size == "small" %} page-width--extra-spacing-column{% endif %}" data-section="{{ section.id }}">
{{ 'section-main-product.css' | asset_url | stylesheet_tag: preload: true }}
{{ 'component-accordion.css' | asset_url | stylesheet_tag }}
{{ 'component-price.css' | asset_url | stylesheet_tag }}
{{ 'component-rating.css' | asset_url | stylesheet_tag }}
{{ 'component-loading-overlay.css' | asset_url | stylesheet_tag }}
{{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}
{%- if product.media.size == 0 -%}
  {{ 'section-main-product-no-media.css' | asset_url | stylesheet_tag }}
{%- endif -%}  

{%- liquid
  unless product.selected_variant
    if section.settings.default_image == 'product_featured_image'
      assign featured_media = product.featured_media  
    else
      if product.selected_or_first_available_variant.featured_media != null
        assign featured_media = product.selected_or_first_available_variant.featured_media
      else
        assign featured_media = product.featured_media  
      endif
    endif  
  else
    if product.selected_variant.featured_media != null
      assign featured_media = product.selected_variant.featured_media
    else
      assign featured_media = product.featured_media  
    endif
  endunless

  assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src'
  assign media_count = product.media.size
  if section.settings.hide_variants and media_count > 1 and variant_images.size > 0
    assign media_count = media_count | minus: variant_images.size | plus: 1
  endif

  if section.settings.media_size == 'large'
    assign media_width = 0.65
  elsif section.settings.media_size == 'medium'
    assign media_width = 0.55
  elsif section.settings.media_size == 'small'
    assign media_width = 0.45
  endif

  assign is_vertical = false
  if section.settings.gallery_layout == 'thumbnail_slider_vertical' and media_count > 1
    assign is_vertical = true
  endif

  assign threshold_media_desktop = 4
  if section.settings.media_size == 'medium'
    assign threshold_media_desktop = 5
  elsif section.settings.media_size == 'large' 
    assign threshold_media_desktop = 6
  endif

  assign has_countdown = false
  assign product_form_id = 'product-form-' | append: section.id

  assign only_use_variant_price = false
  assign price_block = section.blocks | where: "type", "price" | first
  if price_block
    assign only_use_variant_price = price_block.settings.variant_price
  endif
-%}

{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  .breadcrumb {
    padding-bottom: 1.5rem;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
    
    .breadcrumb {
			padding-bottom: 3.2rem;
		}
  }
  {%- if section.settings.enable_sticky_cart -%}
    .sticky-cart-{{ section.id }} {
      --right-position: 1.5rem;
      position: fixed;
      bottom: 1.5rem;
      transform: translateY(calc(100% + 3rem));
      max-width: calc(min(var(--page-width), 100vw) - (var(--right-position) * 2));
    }
  {%- endif -%}
  #MediaGallery-{{ section.id }}{
    --object-fit:{%- if section.settings.image_fit -%}cover{%- else -%}contain{%- endif -%};
  }  
{%- endstyle -%}
<script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>

{%- assign first_3d_model = product.media | where: "media_type", "model" | first -%}
{%- if first_3d_model -%}
  {{ 'component-product-model.css' | asset_url | stylesheet_tag }}
  <link id="ModelViewerStyle" rel="stylesheet" href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css" media="print" onload="this.media='all'">
  <link id="ModelViewerOverride" rel="stylesheet" href="{{ 'component-model-viewer-ui.css' | asset_url }}" media="print" onload="this.media='all'">
{%- endif -%}
  <div class="{% if section.settings.use_background_input %} background-input product-section__padding{% endif %}">
    {%-liquid
      if settings.enable_breadcrumns
        if collection
          render 'breadcrumbs', no_page_width: false, align_classes: 'flex-wrap'
        else
          render 'breadcrumbs', no_page_width: false
        endif  
      endif
    -%}
    <div class="{{ section.settings.section_width }}">
      <div class="product product--{{ section.settings.media_size }} product--media-{{ section.settings.media_position }} {% unless is_vertical %}product--{{ section.settings.gallery_layout }}{% else %}product--thumbnail_slider{% endunless %} product--mobile-{{ section.settings.mobile_thumbnails }} grid grid--1-col grid--gapless {% if product.media.size > 0 %}grid--2-col-tablet{% else %}product--no-media{% endif %} w100">
        <div class="grid__item product__media-wrapper{% if section.settings.enable_sticky_info and section.settings.gallery_layout != 'stacked' and section.settings.gallery_layout != 'columns' %} product__sticky{% endif %}">
          <media-gallery id="MediaGallery-{{ section.id }}" role="region" class="product__media-gallery{% if is_vertical %} product__media-gallery--vertical product__media-gallery--vertical-{{ section.settings.thumbnail_position }}{% endif %}" aria-label="{{ 'products.product.media.gallery_viewer' | t }}" data-desktop-layout="{{ section.settings.gallery_layout }}"{% if section.settings.auto_scroll_up_mobile %} data-scroll-up-mobile="true"{% endif %}{% if section.settings.disable_images_lightbox %} data-disable-lightbox="true"{% endif %}>
            <div id="GalleryStatus-{{ section.id }}" class="visually-hidden" role="status"></div>
            <slider-component id="GalleryViewer-{{ section.id }}" class="slider-mobile-gutter por{% if is_vertical %} product__media-vertical{% endif %}">
              <a class="skip-to-content-link button visually-hidden" href="#ProductInfo-{{ section.id }}">
                <span>{{ "accessibility.skip_to_product_info" | t }}</span>
              </a>
              <ul id="Slider-Gallery-{{ section.id }}" class="product__media-list contains-media grid list-unstyled slider slider--mobile{% if section.settings.gallery_layout != 'stacked' and section.settings.gallery_layout != 'columns' %} slider--tablet slider--desktop slider--everywhere{% endif %}{% if is_vertical %} product__media-list--vertical{% endif %}" role="list">
                {%- if featured_media != null -%}
                  <li id="Slide-{{ section.id }}-{{ featured_media.id }}" class="w100 product__media-item grid__item slider__slide is-active flex-wrap{% if featured_media.media_type != 'image' %} product__media-item--full{% endif %}{% if section.settings.hide_variants and variant_images contains featured_media.src %} product__media-item--variant{% endif %}" data-media-id="{{ section.id }}-{{ featured_media.id }}">
                    {%- assign media_position = 1 -%}
                    {% render 'product-thumbnail', media: featured_media, media_count: media_count, position: media_position, desktop_layout: section.settings.gallery_layout, mobile_layout: section.settings.mobile_thumbnails, loop: section.settings.enable_video_looping, modal_id: section.id, xr_button: true, media_width: media_width, lazy_load: false, image_ratio: section.settings.image_ratio %}
                  </li>
                {%- endif -%}
                {%- for media in product.media -%}
                  {%- unless media.id == featured_media.id -%}
                    <li id="Slide-{{ section.id }}-{{ media.id }}" class="w100 product__media-item grid__item slider__slide flex-wrap{% if product.selected_or_first_available_variant.featured_media == null and forloop.index == 1 %} is-active{% endif %}{% if media.media_type != 'image' %} product__media-item--full{% endif %}{% if section.settings.hide_variants and variant_images contains media.src %} product__media-item--variant{% endif %}" data-media-id="{{ section.id }}-{{ media.id }}">
                      {%- liquid
                        assign media_position = media_position | default: 0 | plus: 1
                        assign lazy_load = false
                        if media_position > 1
                          assign lazy_load = true
                        endif
                        render 'product-thumbnail', media: media, media_count: media_count, position: media_position, desktop_layout: section.settings.gallery_layout, mobile_layout: section.settings.mobile_thumbnails, loop: section.settings.enable_video_looping, modal_id: section.id, xr_button: true, media_width: media_width, lazy_load: lazy_load, image_ratio: section.settings.image_ratio
                      -%}
                    </li>
                  {%- endunless -%}
                {%- endfor -%}
              </ul>
              {%- if media_count > 1 -%}
                <div class="slider-buttons no-js-hidden{% if section.settings.gallery_layout == 'stacked' or section.settings.gallery_layout == 'thumbnail' or section.settings.gallery_layout == 'columns' %} medium-hide large-up-hide{% endif %} flex justify-center align-center">
                  <button type="button" class="product__slider-button slider-button--large product__slider-button--prev slider-button flex flex-column justify-center align-center slider-button--prev tap-area-icon" name="previous" aria-label="{{ 'general.slider.previous_slide' | t }}">{% render 'icon-caret-slider-prev', stroke_width: 2 %}</button>
                  <button type="button" class="product__slider-button slider-button--large product__slider-button--next slider-button flex flex-column justify-center align-center slider-button--next tap-area-icon" name="next" aria-label="{{ 'general.slider.next_slide' | t }}">{% render 'icon-caret-slider-next', stroke_width: 2 %}</button>
                </div>
              {%- endif -%}
            </slider-component>
            {%- if media_count > 1 and section.settings.gallery_layout contains 'thumbnail' or section.settings.mobile_thumbnails == 'show' -%}
              <{%- if is_vertical -%}product-thumbnail-vertical-{%- endif -%}slider-component id="GalleryThumbnails-{{ section.id }}" class="por thumbnail-slider slider-mobile-gutter flex align-center{% if section.settings.mobile_thumbnails != 'show' %} small-hide{% endif %}{% if media_count <= 3 %} thumbnail-slider--no-slide{% endif %}{% if is_vertical %} thumbnail-slider--vertical flex-grow-1{% else %} element-small-margin-top{% endif %}{% if section.settings.gallery_layout contains 'slider' %} slider-component-desktop slider-component-tablet{% endif %} slider-component-mobile{% if section.settings.gallery_layout == 'stacked' or section.settings.gallery_layout == 'columns' %} medium-hide large-up-hide{% endif %}">
                <button type="button" class="product__slider-button--prev slider-button flex flex-column justify-center align-center slider-button--prev tap-area-icon{% if media_count <= 5 %} small-hide{% endif %}{% if section.settings.gallery_layout == 'stacked' or section.settings.gallery_layout == 'thumbnail' or section.settings.gallery_layout == 'columns' %} medium-hide large-up-hide{% else %}{% if media_count <= 4 %} medium-hide{% endif %}{% if media_count <= threshold_media_desktop %} large-up-hide{% endif %}{% endif %}" name="previous" aria-label="{{ 'general.slider.previous_slide' | t }}" aria-controls="GalleryThumbnails-{{ section.id }}">{% render 'icon-caret-slider-prev' %}</button>
                <ul id="Slider-Thumbnails-{{ section.id }}" class="thumbnail-list list-unstyled slider slider--mobile flex align-center{% if section.settings.gallery_layout == 'thumbnail' %} thumbnail-list--grid-on-desktop{% endif %}{% if media_count <= 5 %} thumbnail-list--center-mobile{% endif %}{% unless is_vertical %}{% if media_count <= 4 %} thumbnail-list--center-tablet-up{% elsif media_count <= threshold_media_desktop %} thumbnail-list--center-large-up{% endif %}{% endunless %}{% if section.settings.gallery_layout contains 'thumbnail_slider' %} slider--tablet slider--desktop{% if is_vertical %} thumbnail-list--vertical{% endif %}{% endif %}">
                  {%- if featured_media != null -%}
                    {%- liquid
                      capture media_index
                        if featured_media.media_type == 'model'
                          increment model_index
                        elsif featured_media.media_type == 'video' or featured_media.media_type == 'external_video'
                          increment video_index
                        elsif featured_media.media_type == 'image'
                          increment image_index
                        endif
                      endcapture
                      assign media_index = media_index | plus: 1
                    -%}
                    <li id="Slide-Thumbnails-{{ section.id }}-0" class="thumbnail-list__item slider__slide por{% if section.settings.hide_variants and variant_images contains featured_media.src %} thumbnail-list_item--variant{% endif %}" data-target="{{ section.id }}-{{ featured_media.id }}" data-media-position="{{ media_index }}">
                      {%- if featured_media.media_type == 'model' -%}
                        <span class="thumbnail__badge" aria-hidden="true">
                          {%- render 'icon-3d-model' -%}
                        </span>
                      {%- elsif featured_media.media_type == 'video' or featured_media.media_type == 'external_video' -%}
                        <span class="thumbnail__badge" aria-hidden="true">
                          {%- render 'icon-play' -%}
                        </span>
                      {%- endif -%}
                      <button class="w100 h100 thumbnail global-media-settings--small media {% if section.settings.image_ratio == 'adapt' %}ratio{% else %}media--{{ section.settings.image_ratio }}{% endif %} {% if featured_media.preview_image.aspect_ratio > 1 %}thumbnail--wide{% else %}thumbnail--narrow{% endif %}"
                        aria-label="{%- if featured_media.media_type == 'image' -%}{{ 'products.product.media.load_image' | t: index: media_index }}{%- elsif featured_media.media_type == 'model' -%}{{ 'products.product.media.load_model' | t: index: media_index }}{%- elsif featured_media.media_type == 'video' or featured_media.media_type == 'external_video' -%}{{ 'products.product.media.load_video' | t: index: media_index }}{%- endif -%}"
                        aria-current="true"
                        aria-controls="GalleryViewer-{{ section.id }}"
                        aria-describedby="Thumbnail-{{ section.id }}-0"
                        {% if section.settings.image_ratio == 'adapt' %} style="--ratio-percent:{{ 1 | divided_by: featured_media.preview_image.aspect_ratio | times: 100 }}%;"{% endif %}
                      >
                        <img id="Thumbnail-{{ section.id }}-0" srcset="{% if featured_media.preview_image.width >= 59 %}{{ featured_media.preview_image | image_url: width: 59 }} 59w,{% endif %}
                        {% if featured_media.preview_image.width >= 118 %}{{ featured_media.preview_image | image_url: width: 118 }} 118w,{% endif %}
                        {% if featured_media.preview_image.width >= 84 %}{{ featured_media.preview_image | image_url: width: 84 }} 84w,{% endif %}
                        {% if featured_media.preview_image.width >= 168 %}{{ featured_media.preview_image | image_url: width: 168 }} 168w,{% endif %}
                        {% if featured_media.preview_image.width >= 130 %}{{ featured_media.preview_image | image_url: width: 130 }} 130w,{% endif %}
                        {% if featured_media.preview_image.width >= 260 %}{{ featured_media.preview_image | image_url: width: 260 }} 260w{% endif %}"
                          src="{{ featured_media | image_url: width: 416 }}"
                          sizes="(min-width: {{ settings.page_width }}px) calc(({{ settings.page_width | minus: 100 | times: media_width | round }} - 4rem) / 4), (min-width: 990px) calc(({{ media_width | times: 100 }}vw - 4rem) / 4), (min-width: 750px) calc((100vw - 15rem) / 8), calc((100vw - 14rem) / 3)"
                          alt="{{ featured_media.alt | escape }}"
                          height="208"
                          width="208"
                          loading="lazy"
                        >
                      </button>
                    </li>
                  {%- endif -%}
                  {%- for media in product.media -%}
                    {%- unless media.id == featured_media.id -%}
                      {%- liquid
                        capture media_index
                          if media.media_type == 'model'
                            increment model_index
                          elsif media.media_type == 'video' or media.media_type == 'external_video'
                            increment video_index
                          elsif media.media_type == 'image'
                            increment image_index
                          endif
                        endcapture
                        assign media_index = media_index | plus: 1
                      -%}
                      <li id="Slide-Thumbnails-{{ section.id }}-{{ forloop.index }}" class="thumbnail-list__item slider__slide por{% if section.settings.hide_variants and variant_images contains media.src %} thumbnail-list_item--variant{% endif %}" data-target="{{ section.id }}-{{ media.id }}"  data-media-position="{{ media_index }}">
                        {%- if media.media_type == 'model' -%}
                          <span class="thumbnail__badge" aria-hidden="true">
                            {%- render 'icon-3d-model' -%}
                          </span>
                        {%- elsif media.media_type == 'video' or media.media_type == 'external_video' -%}
                          <span class="thumbnail__badge" aria-hidden="true">
                            {%- render 'icon-play' -%}
                          </span>
                        {%- endif -%}
                        <button class="w100 h100 thumbnail global-media-settings--small media {% if section.settings.image_ratio == 'adapt' %}ratio{% else %}media--{{ section.settings.image_ratio }}{% endif %} {% if media.preview_image.aspect_ratio > 1 %}thumbnail--wide{% else %}thumbnail--narrow{% endif %}"
                          aria-label="{%- if media.media_type == 'image' -%}{{ 'products.product.media.load_image' | t: index: media_index }}{%- elsif media.media_type == 'model' -%}{{ 'products.product.media.load_model' | t: index: media_index }}{%- elsif media.media_type == 'video' or media.media_type == 'external_video' -%}{{ 'products.product.media.load_video' | t: index: media_index }}{%- endif -%}"
                          {% if media == product.selected_or_first_available_variant.featured_media or product.selected_or_first_available_variant.featured_media == null and forloop.index == 1 %} aria-current="true"{% endif %}
                          aria-controls="GalleryViewer-{{ section.id }}"
                          aria-describedby="Thumbnail-{{ section.id }}-{{ forloop.index }}"
                          {% if section.settings.image_ratio == 'adapt' %} style="--ratio-percent:{{ 1 | divided_by: media.preview_image.aspect_ratio | times: 100 }}%;"{% endif %}
                        >
                          <img id="Thumbnail-{{ section.id }}-{{ forloop.index }}"
                            srcset="{% if media.preview_image.width >= 59 %}{{ media.preview_image | image_url: width: 59 }} 59w,{% endif %}
                                    {% if media.preview_image.width >= 118 %}{{ media.preview_image | image_url: width: 118 }} 118w,{% endif %}
                                    {% if media.preview_image.width >= 84 %}{{ media.preview_image | image_url: width: 84 }} 84w,{% endif %}
                                    {% if media.preview_image.width >= 168 %}{{ media.preview_image | image_url: width: 168 }} 168w,{% endif %}
                                    {% if media.preview_image.width >= 130 %}{{ media.preview_image | image_url: width: 130 }} 130w,{% endif %}
                                    {% if media.preview_image.width >= 260 %}{{ media.preview_image | image_url: width: 260 }} 260w{% endif %}"
                            src="{{ media | image_url: width: 84, height: 84 }}"
                            sizes="(min-width: 1200px) calc((1200px - 19.5rem) / 12), (min-width: 750px) calc((100vw - 16.5rem) / 8), calc((100vw - 8rem) / 5)"
                            alt="{{ media.alt | escape }}"
                            height="200"
                            width="200"
                            loading="lazy"
                          >
                        </button>
                      </li>
                    {%- endunless -%}
                  {%- endfor -%}
                </ul>
                <button type="button" class="product__slider-button--next slider-button flex flex-column justify-center align-center slider-button--next tap-area-icon{% if media_count <= 5 %} small-hide{% endif %}{% if section.settings.gallery_layout == 'stacked' or section.settings.gallery_layout == 'thumbnail' or section.settings.gallery_layout == 'columns' %} medium-hide large-up-hide{% else %}{% if media_count <= 4 %} medium-hide{% endif %}{% if media_count <= threshold_media_desktop %} large-up-hide{% endif %}{% endif %}" name="next" aria-label="{{ 'general.slider.next_slide' | t }}" aria-controls="GalleryThumbnails-{{ section.id }}">{% render 'icon-caret-slider-next' %}</button>
              </{%- if is_vertical -%}product-thumbnail-vertical-{%- endif -%}slider-component>
            {%- endif -%}
          </media-gallery>
        </div>
        <div class="product__info-wrapper product__info-wrapper-media-{{ section.settings.media_position }} grid__item por">
          <div id="ProductInfo-{{ section.id }}" class="product__info-container{% if section.settings.enable_sticky_info %}{% if section.settings.gallery_layout == 'stacked' or section.settings.gallery_layout == 'columns' %} product__sticky{% endif %}{% endif %}">
            {%- for block in section.blocks -%}
              {%- case block.type -%}
              {%- when '@app' -%}
                {%- render block -%}
              {%- when 'text' -%}
                {%- if block.settings.text != blank -%}
                  <p class="product__text{% if block.settings.text_style == 'uppercase' %} caption-with-letter-spacing caption-with-letter-spacing--large{% elsif block.settings.text_style == 'subtitle' %} subtitle subtitle--medium{% endif %}{% if block.settings.icon != 'none' %} product__text--with-icon flex{% endif %} break" {{ block.shopify_attributes }}>
                    {%- render 'icon-picker', icon: block.settings.icon, icon_width: 16 -%}
                    <span>{{- block.settings.text -}}</span>
                  </p>
                {%- endif -%}
              {%- when 'title' -%}
                <div class="product__title" {{ block.shopify_attributes }}>
                  {%- if block.settings.show_vendor_link and product.vendor -%}
                    <a href="{{ product.vendor | url_for_vendor }}" class="link color-foreground db w100 product__vendor-link font-body-semi-bold break">{{ 'products.product.by_vendor_html' | t: vendor: product.vendor }}</a>  
                  {%- endif -%}
                  <h1 class="font-heading-extra-bold margin0{% if block.settings.title_size != 'h1' %} {{ block.settings.title_size }}{% endif %}">{{ product.title | escape }}</h1>
                </div>
              {%- when 'price' -%}
                <div id="price-top-{{ section.id }}" {{ block.shopify_attributes }}>
                  {%- unless product.price_varies and only_use_variant_price == false -%}
                    <div class="no-js-hidden" id="price-{{ section.id }}" role="status">
                      {%- render 'price', product: product, use_variant: true, show_badges: true, price_class: 'price--large flex align-center', show_in_card: false -%}
                    </div>
                  {%- else -%}
                    {%- render 'price-varies', target: product -%}  
                  {%- endunless -%}
                </div>
                {%- if cart.taxes_included or shop.shipping_policy.body != blank -%}
                  <div class="product__tax caption rte">
                    {%- if cart.taxes_included -%}
                      {{ 'products.product.include_taxes' | t }}
                    {%- endif -%}
                    {%- if shop.shipping_policy.body != blank -%}
                      {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
                    {%- endif -%}
                  </div>
                {%- endif -%}
                {%- if product != blank -%}
                  <div {{ block.shopify_attributes }}>
                    {%- assign product_form_installment_id = 'product-form-installment-' | append: section.id -%}
                    {%- form 'product', product, id: product_form_installment_id, class: 'installment text-small' -%}
                      <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
                      {{ form | payment_terms }}
                    {%- endform -%}
                  </div>
                {%- endif -%}  
              {%- when 'description' -%}
                {%- render 'main-product-description', product: product, section: section, block: block -%}
              {%- when 'custom_liquid' -%}
                {{ block.settings.custom_liquid }}
              {%- when 'collapsible_tab' -%}
                {%- if request.design_mode or block.settings.content != blank or block.settings.page.content != blank -%}
                  <details-accordion class="product__accordion accordion db" {{ block.shopify_attributes }}>
                    <details id="Details-{{ block.id }}-{{ section.id }}">
                      <summary class="summary-accordion summary-accordion--has-hover flex justify-space-between align-center por">
                        <div class="summary__title flex align-center flex-grow-1">
                          {% render 'icon-accordion', icon: block.settings.icon %}
                          <h2 class="h6 accordion__title margin0 font-heading-bold">
                            {{ block.settings.heading | default: block.settings.page.title }}
                          </h2>
                        </div>
                        <div class="summary-accordion__plus-minus-wrapper flex justify-center align-center">
                          <span class="summary-accordion__plus-minus por flex justify-center align-center">
                            <span class="summary-accordion__icon-line"></span>
                            <span class="summary-accordion__icon-line"></span>
                          </span>
                        </div>
                      </summary>
                      <div class="accordion__content{% if block.settings.icon == 'none' %} accordion__content--no-icon{% endif %}{% if block.settings.content == blank and block.settings.page.content == blank %} accordion__content--empty{% endif %}" id="ProductAccordion-{{ block.id }}-{{ section.id }}">
                        <div class="rte">
                          {{ block.settings.content }}
                          {{ block.settings.page.content }}
                        </div>
                      </div>
                    </details>
                  </details-accordion>
                {%- endif -%}
              {%- when 'popup' -%}
                  <modal-opener class="product-popup-modal__opener no-js-hidden db" data-modal="#PopupModal-{{ block.id }}" {{ block.shopify_attributes }}>
                    <button id="ProductPopup-{{ block.id }}" class="popup-modal__button link underlined-link" type="button" aria-haspopup="dialog">{{ block.settings.text | default: block.settings.page.title }}</button>
                  </modal-opener>
                  <a href="{{ block.settings.page.url }}" class="popup-modal__button link no-js">{{ block.settings.text }}</a>
              {%- when 'share' -%}
                {%- assign share_url = product.selected_variant.url | default: product.url | prepend: request.origin -%}
                {%- render 'main-product-share', share_url: share_url, title: product.title, block: block, featured_media: featured_media -%}
              {%- when 'sale_box' -%}
                <div class="product__sale-box product__info-box flex flex-column{% if block.settings.picker_type == 'button' %} overflow-hidden{% endif %}" {{ block.shopify_attributes }}>
                  {%- render 'main-product-variant-picker', product: product, block: block, product_form_id: product_form_id -%}
                  {%- if product.price_varies and only_use_variant_price == false -%}
                    <div class="no-js-hidden" id="price-{{ section.id }}" role="status">
                      {%- render 'price', product: product, use_variant: true, show_badges: true, price_class: 'price--large flex align-center', show_in_card: false -%}
                    </div>
                  {%- endif -%}
                  {%-liquid
                    if block.settings.show_free_shipping_bar
                      assign should_show_free_shipping_bar = true
                      if settings.free_shipping_exclude_products != blank and settings.free_shipping_exclude_products.count > 0
                        assign exclude_product_ids = settings.free_shipping_exclude_products | map: 'id'
                        if exclude_product_ids contains product.id
                          assign should_show_free_shipping_bar = false
                        endif
                      endif
                      if should_show_free_shipping_bar
                        render 'free-shipping-bar', wrapper_classes: 'product__free-shipping-bar'
                      endif
                    endif
                 -%}
                  <div class="product__sale-box__actions">
                    {%- if product != blank -%}
                      {%- liquid
                        assign gift_card_recipient_feature_active = false
                        if block.settings.show_gift_card_recipient and product.gift_card?
                          assign gift_card_recipient_feature_active = true
                        endif
                  
                        assign show_dynamic_checkout = false
                        if block.settings.show_dynamic_checkout and gift_card_recipient_feature_active == false
                          assign show_dynamic_checkout = true
                        endif
                      -%}
                      <product-form class="product-form" data-hide-errors="{{ gift_card_recipient_feature_active }}">
                        <div class="product-form__error-message-wrapper alert alert--error alert--no-button" role="alert" hidden>
                          <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-error" viewBox="0 0 13 13">
                            <circle cx="6.5" cy="6.50049" r="5.5" stroke="white" stroke-width="2"/>
                            <circle cx="6.5" cy="6.5" r="5.5" fill="currentColor" stroke="currentColor" stroke-width="0.7"/>
                            <path d="M5.87413 3.52832L5.97439 7.57216H7.02713L7.12739 3.52832H5.87413ZM6.50076 9.66091C6.88091 9.66091 7.18169 9.37267 7.18169 9.00504C7.18169 8.63742 6.88091 8.34917 6.50076 8.34917C6.12061 8.34917 5.81982 8.63742 5.81982 9.00504C5.81982 9.37267 6.12061 9.66091 6.50076 9.66091Z" fill="white"/>
                            <path d="M5.87413 3.17832H5.51535L5.52424 3.537L5.6245 7.58083L5.63296 7.92216H5.97439H7.02713H7.36856L7.37702 7.58083L7.47728 3.537L7.48617 3.17832H7.12739H5.87413ZM6.50076 10.0109C7.06121 10.0109 7.5317 9.57872 7.5317 9.00504C7.5317 8.43137 7.06121 7.99918 6.50076 7.99918C5.94031 7.99918 5.46982 8.43137 5.46982 9.00504C5.46982 9.57872 5.94031 10.0109 6.50076 10.0109Z" fill="white" stroke="#EB001B" stroke-width="0.7">
                          </svg>
                          <span class="product-form__error-message"></span>
                        </div>
                        {%- form 'product', product, id: product_form_id, class: 'form', novalidate: 'novalidate', data-type: 'add-to-cart-form' -%}
                          <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}" disabled>
                          {%- if gift_card_recipient_feature_active -%}
                            {%- render 'gift-card-recipient-form', product: product, form: form, section: section -%}
                          {%- endif -%}
                          <div class="product-form__buttons product-form__buttons--input{% unless block.settings.quantity_selector_enable %} product-form__buttons--no-quantity{% endunless %} {% if show_dynamic_checkout and product.selling_plan_groups == empty %}product-form__buttons-group{% else %}product-form__buttons-single{% endif %}">
                            {%- render 'main-product-quantity-selector', product: product, block: block -%}
                            <button
                              type="submit"
                              name="add"
                              class="product-form__submit button flex w100 cart-submit-button button--bold"
                              {% if product.selected_or_first_available_variant.available == false %}disabled{% endif %}
                              id="card-submit-button-{{ section.id }}"
                            >
                              <span>
                                {%- if product.selected_or_first_available_variant.available -%}
                                  {{ 'products.product.add_to_cart' | t }}
                                {%- else -%}
                                  {{ 'products.product.sold_out' | t }}
                                {%- endif -%}
                              </span>
                              <div class="loading-overlay__spinner hidden">
                                {%- render 'icon-cart-loading' -%}
                              </div>
                            </button>
                            {%- if show_dynamic_checkout -%}
                              {{ form | payment_button }}
                            {%- endif -%}
                          </div>
                        {%- endform -%}
                      </product-form> 
                    {%- else -%}
                      <div class="product-form__buttons product-form__buttons--input product-form__buttons-single">
                        <button type="button" name="add" class="product-form__submit button flex w100 cart-submit-button" disabled>
                          <span>
                            {{ 'products.product.sold_out' | t }}
                          </span>
                        </button>
                      </div>
                    {%- endif -%}
                    {{ 'component-pickup-availability.css' | asset_url | stylesheet_tag }}
                    {%- assign pick_up_availabilities = product.selected_or_first_available_variant.store_availabilities | where: 'pick_up_enabled', true -%}

                    <pickup-availability class="product__pickup-availabilities no-js-hidden"
                      {% if product.selected_or_first_available_variant.available and pick_up_availabilities.size > 0 %} available{% endif %}
                      data-root-url="{{ routes.root_url }}"
                      data-variant-id="{{ product.selected_or_first_available_variant.id }}"
                      data-has-only-default-variant="{{ product.has_only_default_variant }}"
                    >
                      <template>
                        <pickup-availability-preview class="pickup-availability-preview">
                          {% render 'icon-unavailable' %}
                          <div class="pickup-availability-info">
                            <p class="text-small">{{ 'products.product.pickup_availability.unavailable' | t }}</p>
                            <button class="pickup-availability-button link link--text underlined-link">{{ 'products.product.pickup_availability.refresh' | t }}</button>
                          </div>
                        </pickup-availability-preview>
                      </template>
                    </pickup-availability>
                  </div>
                  {%-liquid
                    if block.settings.enable_compare and settings.enable_product_compare
                      assign enable_compare = true
                    else
                      assign enable_compare = false
                    endif
                    assign has_size_chart = false
                    if block.settings.size_chart_enable
                      assign size_name = block.settings.size_chart_option
                      if product.options_by_name[size_name] != nil
                        assign has_size_chart = true
                      endif
                    endif
                  -%}
                  {%- if enable_compare or has_size_chart -%}
                    <div class="product__sale-box__footer por flex">
                      {%- if enable_compare -%}
                          {%- render 'product-compare-button-main' -%}
                      {%- endif -%}
                      {%- if has_size_chart -%}
                        {%- if enable_compare -%}
                          <span class="product__sale-box__footer-sep"></span>
                        {%- endif -%}  
                        {%- render 'product-size-chart-button', product: product, block: block -%} 
                      {%- endif -%}  
                    </div>
                  {%- endif -%}  
                </div>
                <script src="{{ 'pickup-availability.js' | asset_url }}" defer="defer"></script>
              {%- when 'rating' -%}
                {%- render 'main-product-rating', product: product, block: block, allow_anchor: true -%}
              {%- when 'complementary' -%}
                {%- render 'complementary-products', product: product, section: section, block: block -%}
              {%- when 'countdown' -%}
                {%- if block.settings.fallback_datetime != blank or block.settings.metafields != blank -%}
                  {%- liquid
                    assign is_countdown_list = false
                    assign final_countdown_value = block.settings.fallback_datetime
                    if block.settings.metafields != blank
                      assign countdown_metafield_namespace_key = block.settings.metafields | split: '.'
                      assign countdown_individual_value = product.metafields[countdown_metafield_namespace_key[0]][countdown_metafield_namespace_key[1]]
                      if countdown_individual_value != blank and countdown_individual_value.type contains 'date'
                        if countdown_individual_value.type contains 'list.'
                          assign is_countdown_list = true
                          assign final_countdown_value = countdown_individual_value.value
                        else
                          assign final_countdown_value = countdown_individual_value.value | date: '%Y-%m-%d %H:%M:%S%z'
                        endif
                      endif  
                    endif  
                  -%}
                  {%- if final_countdown_value != blank -%}
                    {%- assign has_countdown = true -%}
                    <count-down {% if is_countdown_list %}data-list="{% for value in final_countdown_value %}{{ value | date: '%Y-%m-%d %H:%M:%S%z' }}{% unless forloop.last %},{% endunless %}{% endfor %}"{% else %}data-endtime="{{ final_countdown_value }}"{% endif %} data-timezone="{{ 'now' | date: '%z' }}" data-expired-msg="{{ block.settings.expiration_message | escape }}" class="product__info-box flex product__countdown countdown-simple" data-has-own-tpl="true" {{ block.shopify_attributes }}>
                      <div class="countdown-simple__inner flex align-center flex-wrap">
                        {%- if block.settings.title != blank -%}
                          <span class="h5 font-heading-extra-bold">{{ block.settings.title }}</span>
                        {%- endif -%}
                        {%- render 'countdown-tpl-simple' -%}
                      </div>
                    </count-down>
                  {%- endif -%}
                {%- elsif request.design_mode -%}
                  <div class="product__info-box flex product__countdown countdown-simple" {{ block.shopify_attributes }}>
                    <div class="countdown-simple__inner flex align-center flex-wrap">
                      {%- if block.settings.title != blank -%}
                        <span class="h5 font-heading-extra-bold">{{ block.settings.title }}</span>
                      {%- endif -%}
                      {%- render 'countdown-tpl-simple', use_design_mode: true -%}
                    </div>
                  </div>      
                {%- endif -%}
              {%- when 'information_group' -%}
                {%- render 'main-product-information-group', section: section, block: block, product: product -%}
              {%- when 'trusted_payment_badge' -%}
                {%- render 'main-product-trusted-payment-badge', section: section, block: block, product: product -%}
              {%- when 'separated_line' -%}
                <hr {{ block.shopify_attributes }}>
              {%- when 'text_in_multi_line' -%}
                <div class="rte product__rte" {{ block.shopify_attributes }}>
                  {{ block.settings.text }}
                </div>
              {%- endcase -%}
            {%- endfor -%}
          </div>
        </div>
      </div>
    <div>  
  </div>  
  {%- render 'main-product-popups', section: section, product: product -%}
  {%- render 'main-product-size-chart', section: section, product: product -%}
  
  {%- if product.media.size > 0 -%}
    <script src="{{ 'media-gallery.js' | asset_url }}" defer="defer"></script>
    {%- unless section.settings.disable_images_lightbox -%}
      <link rel="stylesheet" href="{{ 'photoswipe.css' | asset_url }}" media="print" onload="this.media='all'">
    {%- endunless -%}
  {%- endif -%}
  {%- if first_3d_model -%}
    <script type="application/json" id="ProductJSON-{{ product.id }}">
      {{ product.media | where: 'media_type', 'model' | json }}
    </script>
    <script src="{{ 'product-model.js' | asset_url }}" defer></script>
  {%- endif -%}
  {%- if has_countdown or request.design_mode -%}
    <script src="{{ 'countdown.js' | asset_url }}" defer="defer"></script>
    <link rel="stylesheet" href="{{ 'component-countdown.css' | asset_url }}" media="print" onload="this.media='all'">
  {%- endif -%}  

  {%- assign collapsible_tabs = section.blocks | where: "type", "collapsible_tab" -%}
  {%- if collapsible_tabs.size > 0 -%}
    <script src="{{ 'details-accordion.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}  

  {%- render 'main-product-seo', product: product, product_form_id: product_form_id -%}
</section>
{%- render 'main-product-sticky-cart', section: section, product: product, product_form_id: product_form_id, featured_media: featured_media -%}
{%- render 'main-product-next-prev', section: section, product: product -%}
{%- if section.settings.enable_shopify_reviews_app_css -%}
  {{ 'component-shopify-product-reviews.css' | asset_url | stylesheet_tag }}
{%- endif -%}
<link rel="stylesheet" href="{{ 'component-rte.css' | asset_url }}" media="print" onload="this.media='all'">
<noscript>{{ 'component-rte.css' | asset_url | stylesheet_tag }}</noscript>
{%- if request.design_mode -%}
  <script src="{{ 'product-price.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'complementary-products.js' | asset_url }}" defer="defer"></script>
{%- endif -%}
